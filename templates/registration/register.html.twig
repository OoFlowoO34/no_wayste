{% extends 'base.html.twig' %}

{% block title %}Register{% endblock %}

{% block body %}
    <h1>Register</h1>

    {# {% for h in home %}
            <li>{{ form_row(h.h_name) }}</li>
    {% endfor %} #}
    <div class="container">
        {{ form_start(registrationForm) }}
        {{ form_row(registrationForm.home) }}
